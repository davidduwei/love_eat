/* pages/editPersonal/editPersonal.wxss */

.userInfo{
   height:200rpx;
  line-height:200rpx;
}
.edit-baseInfo .style-inline{
  padding:0rpx 50rpx;
}

.user-photo-image{
  width: 100%;
  height: 100%;
  border-radius: 50%
}
.user-name{
  padding-left: 40rpx
}
.row-line-personal{
  margin: 20px 0
}
.input-edit{
  width: 50%;
  text-align: center;
  vertical-align:middle;
  height:30px;

}
.picker-input{
  height:30px;
  position:relative;
  top:-15px;

}
.item-head{
  margin:15px 0
}
.checked-item label{
  margin-right: 10px;
}
.checkbox{
  display:inline-block;
  width:20%;
}
.checked-input{
  width:10px
}
.warning-text{
  display: inline-block;
  margin-left: 40px;
  color: red;
}